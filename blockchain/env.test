# Network URLs - Cần thay thế bằng URL thật
SEPOLIA_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
MAINNET_URL=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Private key for deployment (without 0x prefix) - Cần thay thế bằng private key thật
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas reporting
REPORT_GAS=true

# Blockchain service port
BLOCKCHAIN_SERVICE_PORT=5001

# IPFS Configuration (Pinata) - Cần thay thế bằng API key thật
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here

# Test Configuration
NODE_ENV=test 